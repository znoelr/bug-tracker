// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id       String     @id
  username String  @unique
  name     String?
}

model Task {
  id       String     @id
  title String  @unique
  description String
  priority     String
}
