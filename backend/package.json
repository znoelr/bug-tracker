{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "prisma": {
    "schema": "src/infrastructure/prisma/schema.prisma",
    "seed": "ts-node ./src/scripts/prisma-seed/seed/index.ts"
  },
  "scripts": {
    "build": "tsc -b",
    "test": "docker compose -f test.docker-compose.yaml up --build",
    "dev": "docker compose up",
    "clear:test": "docker compose -f test.docker-compose.yaml down",
    "start:test": "npx prisma generate && dotenv -e .env.test -- npx prisma db push --force-reset --accept-data-loss && dotenv -e .env.test -- jest --watchAll",
    "start:dev": "dotenv -e .env.development -- npx tsx --watch --trace-warnings src/server.ts",
    "db:migrate": "dotenv -e .env.development -- npx prisma migrate dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "dotenv-cli": "^7.3.0",
    "express": "^4.18.2",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.2",
    "pino-http": "^9.0.0",
    "pino-pretty": "^10.3.1",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.2.1",
    "sonic-boom": "^3.8.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/pino-http": "^5.8.4",
    "@types/sonic-boom": "^2.1.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "jest": "^29.7.0",
    "prisma": "^5.10.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2"
  }
}
